<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	android:id="@+id/widget0"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">

<ImageView
    android:id="@+id/qnprofilepic"
    android:layout_width="93dp"
    android:layout_height="114dp"
    android:layout_x="1dp"
    android:layout_y="1dp"
    android:src="@drawable/minion" />

<TextView
	android:id="@+id/widget50"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="Hi JonathanTan!"
	android:textSize="18sp"
	android:textStyle="bold"
	android:textColor="#ff000000"
	android:layout_x="129dp"
	android:layout_y="34dp" />
<Button
	android:id="@+id/FAQ"
	android:layout_width="74dp"
	android:layout_height="37dp"
	android:text="FAQ"
	android:textStyle="bold"
	android:layout_x="123dp"
	android:layout_y="79dp" />
<Button
	android:id="@+id/Logout"
	android:layout_width="81dp"
	android:layout_height="38dp"
	android:text="Logout"
	android:textStyle="bold"
	android:textColor="#ff000000"
	android:layout_x="212dp"
	android:layout_y="79dp" />
<TextView
	android:id="@+id/EditProfile"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="Edit Profile"
	android:textSize="25sp"
	android:textStyle="bold"
	android:textColor="#ff000000"
	android:layout_x="4dp"
	android:layout_y="120dp" />

<ImageButton
    

    android:id="@+id/Asking"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_x="12dp"
    android:layout_y="362dp"
    android:src="@drawable/as" />

<ImageButton
    android:id="@+id/notification"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginLeft="70dp"
    android:layout_marginRight="70dp"
    android:layout_x="83dp"
    android:layout_y="362dp"
    android:src="@drawable/noti" />


<TextView
    android:id="@+id/count"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="127dp"
    android:layout_y="363dp"
    android:background="@drawable/notificationred"
    android:gravity="center"
    android:text="10"
    android:textColor="@color/text"
    android:textSize="12dp"
    android:textStyle="bold" />


<ImageButton
    android:id="@+id/QuestionBank"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_x="154dp"
    android:layout_y="362dp"
    android:src="@drawable/myquestionbank" />

<ImageButton
    android:id="@+id/setting"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_x="225dp"
    android:layout_y="362dp"
    android:src="@drawable/settings" />

<TextView
    android:id="@+id/textView1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="42dp"
    android:layout_y="161dp"
    android:textColor="#ff000000"
    android:text="Profile Picture" />

<ImageButton
    android:id="@+id/imageButton1"
    android:layout_width="89dp"
    android:layout_height="110dp"
    android:layout_x="46dp"
    android:layout_y="185dp"
    android:src="@drawable/uploadprofilepic" />

<Button
    android:id="@+id/Confirm"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="29dp"
    android:layout_y="304dp"
    android:text="Confirm" />

<Button
    android:id="@+id/Cancel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="137dp"
    android:layout_y="304dp"
    android:text="Cancel" />

</AbsoluteLayout>
