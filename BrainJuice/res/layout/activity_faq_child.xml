<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	android:id="@+id/widget0"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">

<ImageView
    android:id="@+id/qnprofilepic"
    android:layout_width="70dp"
    android:layout_height="70dp"
    android:layout_x="7dp"
    android:layout_y="5dp"
    android:foreground="#152"
    android:src="@drawable/minion" />

<TextView
    android:id="@+id/widget50"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="87dp"
    android:layout_y="12dp"
    android:text="Hi JonathanTan!"
    android:textColor="#ff000000"
    android:textSize="18sp"
    android:textStyle="bold"
    android:fontFamily="" />

<ImageButton
    android:id="@+id/FAQ"
    android:layout_width="80dp"
    android:layout_height="30dp"
    android:layout_x="81dp"
    android:layout_y="42dp"
    android:src="@drawable/faq_button" />

<ImageButton
    android:id="@+id/Logout"
    android:layout_width="80dp"
    android:layout_height="30dp"
    android:layout_x="168dp"
    android:layout_y="43dp"
    android:src="@drawable/logout_button" />

<ImageButton
    

    android:id="@+id/Asking"
    android:layout_width="90dp"
    android:layout_height="60dp"
    android:layout_x="-4dp"
    android:layout_y="405dp"
    android:src="@drawable/as" />

<ImageButton
    android:id="@+id/notification"
     android:layout_width="90dp"
    android:layout_height="60dp"
    android:layout_marginLeft="70dp"
    android:layout_marginRight="70dp"
    android:layout_x="79dp"
    android:layout_y="405dp"
    android:src="@drawable/noti" />

<TextView
    android:id="@+id/count"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="123dp"
    android:layout_y="405dp"
    android:background="@drawable/notificationred"
    android:gravity="center"
    android:text="10"
    android:textColor="@color/text"
    android:textSize="12dp"
    android:textStyle="bold" />


<ImageButton
    android:id="@+id/QuestionBank"
       android:layout_width="90dp"
    android:layout_height="60dp"
    android:layout_x="161.31dp"
    android:layout_y="405dp"
    android:src="@drawable/myquestionbank" />

<ImageButton
    android:id="@+id/Setting"
    android:layout_width="90dp"
    android:layout_height="60dp"
    android:layout_x="242.5dp"
    android:layout_y="405dp"
    android:src="@drawable/settings" />

<ScrollView
    android:id="@+id/scrollView1"
    android:layout_width="286dp"
    android:layout_height="267dp"
    android:layout_x="15dp"
    android:layout_y="132dp" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >
        
        <TextView
		    android:id="@+id/Q1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="31dp"
		    android:layout_y="165dp"
		    android:text="How do I ask question?" />
        
        <TextView
		    android:id="@+id/Q2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="29dp"
		    android:layout_y="192dp"
		    android:text="How do I know if my question has been answered?" />
        
        <TextView
		    android:id="@+id/Q3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="29dp"
		    android:layout_y="192dp"
		    android:text="Where are my answered questions located?" />
        
        <TextView
		    android:id="@+id/Q4"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="29dp"
		    android:layout_y="192dp"
		    android:text="Where can I edit my profile?" />
        
        <TextView
		    android:id="@+id/Q5"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_x="29dp"
		    android:layout_y="192dp"
		    android:text="Where can I change my password?" />
        
        
    </LinearLayout>
</ScrollView>

<ImageView
    android:id="@+id/imageView1"
    android:layout_width="80dp"
    android:layout_height="47dp"
    android:layout_x="4dp"
    android:layout_y="82dp"
    android:src="@drawable/faqchild" />

</AbsoluteLayout>
