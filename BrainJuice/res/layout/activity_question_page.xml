	android:textStyle="bold"
	android:textColor="#ff000000"
	android:layout_x="212dp"
	android:layout_y="79dp" />
<TextView
	android:id="@+id/AskQuestion"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="Ask Question"
	android:textSize="25sp"
	android:textStyle="bold"
	android:textColor="#ff000000"
	android:layout_x="4dp"
	android:layout_y="120dp" />
<TableLayout
	android:id="@+id/widget57"
	android:layout_width="258dp"
	android:layout_height="145dp"
	android:padding="10dp"
	android:orientation="vertical"
	android:layout_x="33dp"
	android:layout_y="165dp">

    <EditText
        android:id="@+id/editText1"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:ems="10" >

        <requestFocus />
    </EditText>

</TableLayout>
<Button



	android:id="@+id/Ask"
	android:layout_width="61dp"
	android:layout_height="34dp"
	android:text="Ask"
	android:textStyle="bold"
	android:layout_x="235dp"
	android:layout_y="323dp" />

<ImageButton
    

    android:id="@+id/Asking"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_x="12dp"
    android:layout_y="362dp"
    android:src="@drawable/as" />

<ImageButton
    android:id="@+id/notification"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginLeft="70dp"
    android:layout_marginRight="70dp"
    android:layout_x="83dp"
    android:layout_y="362dp"
    android:src="@drawable/noti" />

<TextView
    android:id="@+id/count"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="127dp"
    android:layout_y="363dp"
    android:background="@drawable/notificationred"
    android:gravity="center"
    android:text="10"
    android:textColor="@color/text"
    android:textSize="12dp"
    android:textStyle="bold" />


<ImageButton
    android:id="@+id/widget42"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_x="154dp"
    android:layout_y="362dp"
    android:src="@drawable/myquestionbank" />

<ImageButton
    android:id="@+id/widget43"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_x="225dp"
    android:layout_y="362dp"
    android:src="@drawable/settings" />

</AbsoluteLayout>
